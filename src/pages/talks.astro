---
import BaseLayout from '../layouts/BaseLayout.astro';
import { getCollection } from 'astro:content';
const talks = Object.values(import.meta.glob('./talks/*.md', { eager: true }));

---

<p>attended talks:</p>
<ul>
  {talks.map(post => <li><a href={post.url}>{post.frontmatter.title}</a></li>)}
</ul>
